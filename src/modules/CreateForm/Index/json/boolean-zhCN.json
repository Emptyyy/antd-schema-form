{
  "id": "$root",
  "type": "object",
  "title": "boolean类型",
  "properties": {
    "id": {
      "id": "$root/properties/id",
      "type": "string",
      "title": "当前属性的id",
      "$required": true
    },
    "type": {
      "id": "$root/properties/type",
      "type": "string",
      "title": "数据类型",
      "$required": true,
      "$readOnly": true,
      "$defaultValue": "boolean"
    },
    "title": {
      "id": "$root/properties/title",
      "type": "string",
      "title": "标题，用于描述关键字的作用",
      "$required": true
    },
    "description": {
      "id": "$root/properties/description",
      "type": "string",
      "title": "说明，用于描述关键字的作用"
    },
    "$hidden": {
      "id": "$root/properties/$hidden",
      "type": "boolean",
      "title": "隐藏表单域（表单值仍然存在）"
    },
    "$tableColumnHidden": {
      "id": "$root/properties/$tableColumnHidden",
      "type": "boolean",
      "title": "为数组内的对象且组件为表格时，隐藏列（表单值仍然存在）"
    },
    "$defaultValue": {
      "id": "$root/properties/$defaultValue",
      "type": "boolean",
      "title": "表单的默认值"
    },
    "$componentType": {
      "id": "$root/properties/$componentType",
      "type": "string",
      "title": "渲染为其他组件",
      "$componentType": "select",
      "$options": [
        {
          "label": "开关",
          "value": "switch"
        }
      ]
    }
  }
}