{
  "name": "antd-schema-form",
  "version": "3.0.1",
  "description": "基于antd，通过json schema配置表单。",
  "main": "lib/index.js",
  "module": "es/index.js",
  "author": {
    "name": "段昊辰",
    "email": "duanhaochen@126.com",
    "url": "https://github.com/duan602728596"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:duan602728596/antd-schema-form.git"
  },
  "homepage": "https://duan602728596.github.io/antd-schema-form/#/",
  "license": "MIT",
  "keywords": [
    "form",
    "antd",
    "react-component"
  ],
  "scripts": {
    "dev": "webpack-dev-server --config=\"./config/webpack.dev.js\" --hot --color --port=5050",
    "build": "npm run clean:react-slick && npm run clean:lib && npm run clean:es && npm run clean:style && cross-env NODE_ENV=production gulp",
    "clean:lib": "rimraf lib",
    "clean:es": "rimraf es",
    "clean:style": "rimraf style",
    "clean:react-slick": "rimraf node_modules/@types/react-slick/node_modules",
    "test:init": "mocha init tests",
    "test": "webpack-dev-server --config=\"./config/webpack.test.js\" --hot --color --port=6060",
    "eslint": "cross-env NODE_ENV=eslint eslint ./ --ext .js,.jsx,.ts,.tsx --fix",
    "case-check": "case-check --dir=\"./\" --ext=\"sass\"",
    "update": "milktea update"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "immutability-helper": "^3.0.1",
    "lodash-es": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/plugin-proposal-do-expressions": "^7.6.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-react": "^7.7.0",
    "@babel/runtime-corejs3": "^7.7.2",
    "@sweet-milktea/milktea": "^0.6.6",
    "@sweet-milktea/milktea-cli": "^0.6.6",
    "@sweet-milktea/util-tools": "^0.6.6",
    "@types/react": "^16.9.11",
    "@typescript-eslint/eslint-plugin-tslint": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "antd": "^4.0.0-alpha.9",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.2",
    "chai": "^4.2.0",
    "commitizen": "^4.0.3",
    "core-js": "^3.4.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "cz-conventional-changelog": "^3.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "es-module-path-case-check": "^1.4.0",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.15.1",
    "file-loader": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-filter": "^6.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "merge2": "^1.3.0",
    "mocha": "^6.2.2",
    "node-sass": "^4.13.0",
    "rc-field-form": "^0.0.0-alpha.23",
    "rc-table": "^7.0.0-alpha.20",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "peerDependencies": {
    "antd": ">=3.12.0",
    "react": ">=16.7.0",
    "react-dom": ">=16.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
