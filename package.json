    "@sweet/util-tools": "github:duan602728596/sweet#build-util-tools",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.11.0",
    "chai": "^4.2.0",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.8.0",
    "eslint-plugin-flowtype": "^3.2.0",
