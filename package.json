{
  "name": "antd-schema-form-website-v3",
  "description": "antd-schema-form的在线演示地址的源代码。",
  "scripts": {
    "dll": "milktea dll",
    "start": "cross-env NODE_ENV=development milktea start --server",
    "build": "npm run clean && cross-env NODE_ENV=production milktea build",
    "clean": "rimraf ./dist",
    "server": "milktea server",
    "eslint": "eslint ./src --fix",
    "case-check": "case-check --dir=\"./\" --css",
    "update": "milktea update"
  },
  "dependencies": {
    "@ant-design/icons": "^2.1.1",
    "antd": "^3.20.3",
    "antd-schema-form": "^2.4.2",
    "classnames": "^2.2.6",
    "highlight.js": "^9.15.8",
    "immutable": "^4.0.0-rc.12",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@sweet-milktea/milktea": "^0.3.6",
    "@sweet-milktea/milktea-cli": "^0.3.5",
    "@sweet-milktea/server": "^0.3.6",
    "@sweet-milktea/util-tools": "^0.3.6",
    "babel-eslint": "^10.0.2",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-import-components-style": "^0.1.0",
    "commitizen": "^4.0.1",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "es-module-path-case-check": "^1.4.0",
    "eslint": "^6.0.1",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
