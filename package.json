{
  "name": "antd-schema-form-website",
  "description": "antd-schema-form的在线演示地址的源代码。",
  "scripts": {
    "dll": "milktea dll",
    "start": "cross-env NODE_ENV=development milktea start --server",
    "build": "npm run clean && cross-env NODE_ENV=production milktea build",
    "clean": "rimraf ./build",
    "server": "milktea server",
    "eslint": "eslint ./ --fix",
    "update": "milktea update"
  },
  "devDependencies": {
    "@svgr/webpack": "^4.1.0",
    "@sweet/milktea": "github:duan602728596/sweet#build-milktea",
    "@sweet/milktea-cli": "github:duan602728596/sweet#build-milktea-cli",
    "@sweet/server": "github:duan602728596/sweet#build-server",
    "@sweet/util-tools": "github:duan602728596/sweet#build-util-tools",
    "antd": "^3.15.0",
    "antd-schema-form": "^2.0.0-beta.12",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.11.0",
    "classnames": "^2.2.6",
    "commitizen": "^3.0.7",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.15.1",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-react": "^7.12.4",
    "highlightjs": "^9.12.0",
    "husky": "^1.3.1",
    "immutable": "^4.0.0-rc.12",
    "lint-staged": "^8.1.5",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.4.0-beta.7",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rimraf": "^2.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
