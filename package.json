{
  "name": "antd-schema-form-website-v4",
  "description": "antd-schema-form的在线演示地址的源代码。",
  "scripts": {
    "dll": "milktea dll",
    "start": "cross-env NODE_ENV=development milktea start --server",
    "build": "npm run clean && cross-env NODE_ENV=production milktea build",
    "clean": "rimraf ./dist",
    "server": "milktea server",
    "eslint": "eslint ./src --fix",
    "case-check": "case-check --dir=\"./\" --css",
    "update": "milktea update"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.5",
    "antd": "^4.1.1",
    "antd-schema-form": "^4.0.0",
    "classnames": "^2.2.6",
    "highlight.js": "^9.18.1",
    "immutable": "^4.0.0-rc.12",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.2.0",
    "react-router": "^6.0.0-alpha.3",
    "react-router-dom": "^6.0.0-alpha.3",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@sweet-milktea/milktea": "^0.15.0",
    "@sweet-milktea/milktea-cli": "^0.14.0",
    "@sweet-milktea/server": "^0.15.0",
    "@sweet-milktea/util-tools": "^0.15.0",
    "babel-eslint": "^11.0.0-beta.2",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-import-components-style": "^0.1.0",
    "commitizen": "^4.0.4",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.1.0",
    "es-module-path-case-check": "^1.5.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
