{
  "name": "antd-schema-form-website",
  "description": "antd-schema-form的在线演示地址的源代码。",
  "scripts": {
    "dll": "milktea dll",
    "start": "cross-env NODE_ENV=development milktea start --server",
    "build": "npm run clean && cross-env NODE_ENV=production milktea build",
    "clean": "rimraf ./build",
    "server": "milktea server",
    "eslint": "eslint ./src --fix",
    "case-check": "case-check --dir=\"./\" --css",
    "update": "milktea update"
  },
  "devDependencies": {
    "@sweet-milktea/milktea": "^0.2.0",
    "@sweet-milktea/milktea-cli": "^0.1.1",
    "@sweet-milktea/server": "^0.2.0",
    "@sweet-milktea/util-tools": "^0.0.1",
    "antd": "^3.19.5",
    "antd-schema-form": "^2.3.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-import-components-style": "^0.1.0",
    "classnames": "^2.2.6",
    "commitizen": "^3.1.1",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "es-module-path-case-check": "^1.4.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "highlightjs": "^9.12.0",
    "husky": "^2.4.1",
    "immutable": "^4.0.0-rc.12",
    "lint-staged": "^8.2.1",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rimraf": "^2.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
